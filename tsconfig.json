{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "baseUrl": "./",
    "paths": {
	"@solana/web3.js": ["node_modules/@solana/web3.js"]
}
  },
  "exclude": [
    "build/*",
    "node_modules/*"
  ]
}
